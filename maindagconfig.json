{
  "environment": "development",
  "api_credentials": {
    "project_id": "your-gcp-project-id",
    "client_id": "your-client-id",
    "client_secret": "your-client-secret",
    "secret_manager_client_id": "your-secret-manager-client-id",
    "secret_manager_client_secret": "your-secret-manager-client-secret"
  },
  "api_endpoints": {
    "auth_url": "your-auth-url",
    "scope_url": "your-scope-url"
  },
  "gcs_bucket": "your-gcs-bucket-name",
  "destination_blob_name": "data",
  "bigquery_table": "your-gcp-project-id.your_dataset.your_table",
  "main_dag_properties": {
    "owner": "main_dag_owner",
    "depends_on_past": false,
    "start_date": "2023-11-01T00:00:00",
    "email": ["main_dag_email@example.com"],
    "email_on_failure": true,
    "email_on_retry": false,
    "retries": 1,
    "retry_delay_minutes": 5,
    "schedule_interval": "@daily",
    "catchup": false,
    "tags": ["main_dag_tag"],
    "max_active_runs": 1
  }
}
